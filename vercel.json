{
  "version": 2,
  "builds": [
    {
      "src": "api/render.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/render",
      "dest": "/api/render.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "functions": {
    "api/render.ts": {
      "maxDuration": 10
    }
  }
}